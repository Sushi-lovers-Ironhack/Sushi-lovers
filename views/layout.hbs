<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="/stylesheets/reset.css" rel="stylesheet" />
    <link href="/stylesheets/style.css" rel="stylesheet" />
    <link href="/stylesheets/navbar.css" rel="stylesheet" />
    <title>Project module 2</title>
  </head>
  <nav>
    <section id="nav-bar">
      <section id="nav-menu">
        <img
          src="data:image/png;base64,"
          alt="menu"
          class="nav-buttons"
        />
        <ul class="dropdown">
          {{#if name}}
            <li><a href="/restaurant">Home</a></li>
            <li><a href="/restaurant/profile">Account</a></li>
            <li><a href="/auth/logout">Log out</a></li>
            <li><a href="">Support</a></li>
          {{else}}
            {{#if username}}
              <li><a href="/">Home</a></li>
              <li><a href="/user/profile">Account</a></li>
              <li><a href="/auth/logout">Logout</a></li>
              <li><a href="">Support</a></li>
            {{else}}
              <li><a href="/">Home</a></li>
              <li><a href="/auth/signup">Sign up User</a></li>
              <li><a href="/auth/restaurant/signup">Sign up Restaurant</a></li>
              <li><a href="/auth/login">Log In</a></li>
              <li><a href="">Contact us</a></li>
            {{/if}}
          {{/if}}
        </ul>
      </section>
      {{#if orderActive}}
        <a href="/cart/status">See your orders</a>
      {{/if}}
      <button onclick="history.back()"><img
          src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRIXKTpmW8n9pwEfKo_ct4njXJAFSXJfocxWg&usqp=CAU"
          alt="Go back"
          class="nav-buttons go-back"
        /></button>
    </section>
